{
  "expo": {
    "name": "Mykids",
    "slug": "my-kids",
    "version": "10.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#499DA7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "MyKids.ma"
    },
    "android": {
      "package": "MyKids.ma",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icon2.png",
        "backgroundColor": "#3B7C88"
      },
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    }
  }
}